{
  "buildCommand": "npm run build --prefix nest-flow",
  "outputDirectory": "nest-flow/dist",
  "installCommand": "npm install --prefix nest-flow",
  "devCommand": "npm run dev --prefix nest-flow",
  "framework": "vite",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

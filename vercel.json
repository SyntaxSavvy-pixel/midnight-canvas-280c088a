{
  "version": 2,
  "name": "tabmanagement",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.ico",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/mainlandingpage.html"
    },
    {
      "src": "/login",
      "dest": "/New-authentication.html"
    },
    {
      "src": "/signup",
      "dest": "/New-authentication.html?mode=signup"
    },
    {
      "src": "/dashboard",
      "dest": "/user-dashboard.html"
    },
    {
      "src": "/auth",
      "dest": "/New-authentication.html"
    },
    {
      "src": "/checkout",
      "dest": "/payment-checkout.html"
    },
    {
      "src": "/returning",
      "dest": "/Returning-Users.html"
    },
    {
      "src": "/pricing",
      "dest": "/mainlandingpage.html#pricing"
    },
    {
      "src": "/features",
      "dest": "/mainlandingpage.html#features"
    },
    {
      "src": "/faq",
      "dest": "/mainlandingpage.html#faq"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/icons/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend-api.vercel.app/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/landing",
      "destination": "/",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
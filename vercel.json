{
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/mainlandingpage.html"
    },
    {
      "source": "/login",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/signup",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/dashboard",
      "destination": "/user-dashboard.html"
    },
    {
      "source": "/auth",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/checkout",
      "destination": "/payment-checkout.html"
    },
    {
      "source": "/returning",
      "destination": "/Returning-Users.html"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/"
    },
    {
      "source": "/landing",
      "destination": "/"
    },
    {
      "source": "/pricing",
      "destination": "/mainlandingpage.html#pricing"
    },
    {
      "source": "/features",
      "destination": "/mainlandingpage.html#features"
    },
    {
      "source": "/faq",
      "destination": "/mainlandingpage.html#faq"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/mainlandingpage.html"
    },
    {
      "src": "^/login/?$",
      "dest": "/New-authentication.html"
    },
    {
      "src": "^/signup/?$",
      "dest": "/New-authentication.html"
    },
    {
      "src": "^/dashboard/?$",
      "dest": "/user-dashboard.html"
    },
    {
      "src": "^/auth/?$",
      "dest": "/New-authentication.html"
    },
    {
      "src": "^/checkout/?$",
      "dest": "/payment-checkout.html"
    },
    {
      "src": "^/returning/?$",
      "dest": "/Returning-Users.html"
    },
    {
      "src": "^/home/?$",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "^/landing/?$",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "^/pricing/?$",
      "status": 301,
      "headers": { "Location": "/mainlandingpage.html#pricing" }
    },
    {
      "src": "^/features/?$",
      "status": 301,
      "headers": { "Location": "/mainlandingpage.html#features" }
    },
    {
      "src": "^/faq/?$",
      "status": 301,
      "headers": { "Location": "/mainlandingpage.html#faq" }
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
      },
      "dest": "/$1"
    }
  ]
}
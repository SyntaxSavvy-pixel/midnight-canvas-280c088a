{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/mainlandingpage.html"
    },
    {
      "source": "/login",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/signup",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/dashboard",
      "destination": "/user-dashboard.html"
    },
    {
      "source": "/auth",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/checkout",
      "destination": "/payment-checkout.html"
    },
    {
      "source": "/returning",
      "destination": "/Returning-Users.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend-api.vercel.app/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/landing",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/pricing",
      "destination": "/mainlandingpage.html#pricing",
      "permanent": false
    },
    {
      "source": "/features",
      "destination": "/mainlandingpage.html#features",
      "permanent": false
    },
    {
      "source": "/faq",
      "destination": "/mainlandingpage.html#faq",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/icons/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
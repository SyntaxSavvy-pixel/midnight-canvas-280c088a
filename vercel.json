{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node@3"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/mainlandingpage.html"
    },
    {
      "source": "/login",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/signup",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/dashboard",
      "destination": "/user-dashboard.html"
    },
    {
      "source": "/auth",
      "destination": "/New-authentication.html"
    },
    {
      "source": "/checkout",
      "destination": "/payment-checkout.html"
    },
    {
      "source": "/returning",
      "destination": "/Returning-Users.html"
    }
  ]
}